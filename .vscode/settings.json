{
    "workbench.colorCustomizations": {
        "activityBar.background": "#0E350C",
        "titleBar.activeBackground": "#144B10",
        "titleBar.activeForeground": "#F3FCF3"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.formatOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "vue"
    ]
}